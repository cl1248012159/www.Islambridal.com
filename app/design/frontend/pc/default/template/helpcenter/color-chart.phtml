    function loadImage(url, callback, id) {
        var img = new Image();
        img.src = url;
        if (img.complete) {
            callback.call(img, id);
        } else {
            jQuery(img).bind("load",
                function() {
                    callback.call(img, id);
                });
        }
    }
    for (var obj in list) {
        var str = '';
        var y = 0;
        var rows = 0;
        for (i = 0; i < list[obj].length; i++) {
            var cols = i % 9;
            if (cols == 8) {
                rows++;
            }
            var x = (32 + 36) * cols + 15;
            var x2 = x + 1;
            str += '<div to="' + list[obj][i].img + '" title="' + list[obj][i].name + '" style="left:' + (x + 1) + 'px;top:' + (y + 1) + 'px"></div>';
            y = (20 + 36) * rows;
        }
        jQuery('#' + obj).html(jQuery('#' + obj).html() + str);

        jQuery('#' + obj + ' div').each(function(n) {
            jQuery(this).css({
                opacity: 0,
                'background-color': '#fff'
            })
            jQuery(this).hover(function() {
                jQuery(this).css({
                    opacity: 0.8
                }).animate({
                        opacity: 0
                    },
                    600);
            })

            jQuery(this).click(function() {
                var title = jQuery(this).attr('title');
                var url = '<?php echo $this->getSkinUrl('')?>images/color/' + jQuery(this).attr('to') + '.jpg';
